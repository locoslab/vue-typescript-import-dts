{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"sourceMap": false,
		"pretty": true,
		"outDir": "ignore",
		"lib": ["es5", "dom", "es2015.promise"]
	},
	"exclude": [
		"node_modules"
	],
	"atom": {
		"rewriteTsconfig": false
	},
	"compileOnSave": false
}
